    <SolidColorBrush x:Key="ListBoxItemDisabledBackground" Color="Transparent" />
    <StaticResource x:Key="ListBoxItemSelectedDisabledBackground" ResourceKey="SemiColorDisabledFill" />
</ResourceDictionary>