    <StaticResource x:Key="SliderTrackBackground" ResourceKey="SemiColorFill0" />
    <SolidColorBrush x:Key="SliderTickForeground" Color="#C6CACD" />
    <StaticResource x:Key="SliderThumbBackground" ResourceKey="SemiColorWhite" />
